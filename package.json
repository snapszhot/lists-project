{
    "name": "lists-project",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "release": "dotenv -e .env.local release-it"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.0.5",
        "@dnd-kit/modifiers": "^6.0.0",
        "@dnd-kit/sortable": "^7.0.1",
        "@dnd-kit/utilities": "^3.2.0",
        "@prismicio/client": "^6.7.1",
        "@prismicio/react": "^2.5.0",
        "@sentry/nextjs": "^7.17.2",
        "@supabase/supabase-js": "^2.0.4",
        "axios": "^1.1.3",
        "classnames": "^2.3.2",
        "cookie": "^0.5.0",
        "js-file-download": "^0.4.12",
        "json2csv": "^5.0.7",
        "lodash.debounce": "^4.0.8",
        "luxon": "^3.0.4",
        "next": "^13.0.0",
        "next-seo": "^5.11.1",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.38.0",
        "slugify": "^1.6.5"
    },
    "devDependencies": {
        "dotenv-cli": "^6.0.0",
        "eslint": "^8.26.0",
        "eslint-config-next": "^13.0.0",
        "eslint-config-prettier": "^8.5.0",
        "sass": "^1.55.0"
    },
    "release-it": {
        "git": {
            "changelog": "npx auto-changelog --stdout --commit-limit false --unreleased --template https://raw.githubusercontent.com/release-it/release-it/master/templates/changelog-compact.hbs",
            "commitMessage": "Releasing v${version}",
            "tagName": "v${version}"
        },
        "github": {
            "release": true,
            "releaseName": "v${version}",
            "tokenRef": "GITHUB_TOKEN"
        },
        "hooks": {
            "after:bump": "npx auto-changelog -p --template changelog-template.hbs --ignore-commit-pattern \"^(Releasing v.*)\""
        },
        "npm": {
            "publish": false
        }
    }
}
